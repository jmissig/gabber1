	  <icon>glade-unlock-key.xpm</icon>
	  <active>False</active>
	</widget>

	<widget>
	  <class>GtkButton</class>
	  <child_name>Toolbar:button</child_name>
	  <name>OOOChat_AddUser_btn</name>
