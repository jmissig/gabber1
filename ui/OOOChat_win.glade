	<widget>
	  <class>GtkToggleButton</class>
	  <child_name>Toolbar:button</child_name>
	  <name>OOOChat_Message_chk</name>
	  <tooltip>Send as Normal Message</tooltip>
	  <label>Send as Normal Message</label>
	  <icon>glade-sendasnormal.xpm</icon>
	  <active>False</active>
	</widget>

	<widget>
	  <class>GtkToggleButton</class>
	  <child_name>Toolbar:button</child_name>
	  <name>OOOChat_Encrypt_tglbtn</name>
	  <tooltip>Encrypt this message</tooltip>
